<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time/"
     xml:base="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.w3.org/2000/01/rdf-schema#ChineseName -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#ChineseName"/>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#symbols -->

    <owl:AnnotationProperty rdf:about="http://www.w3.org/2000/01/rdf-schema#symbols"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Age -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Age">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Interval"/>
        <rdfs:ChineseName xml:lang="zh">期</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Anisian_Age -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Anisian_Age">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Age"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Middle_Triassic"/>
        <rdfs:ChineseName>安尼期</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Cambrian -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Cambrian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Paleozoic"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Period"/>
        <rdfs:ChineseName xml:lang="zh-cn">寒武纪</rdfs:ChineseName>
        <rdfs:comment>距今约5.41亿年前－4.854亿年前</rdfs:comment>
        <rdfs:symbols>Ꞓ</rdfs:symbols>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Carboniferous -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Carboniferous">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Paleozoic"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Period"/>
        <rdfs:ChineseName>石炭纪</rdfs:ChineseName>
        <rdfs:comment>它从3.58亿年前（358百万年前）开始，延续到2.98亿年前（298百万年前）</rdfs:comment>
        <rdfs:symbols>C</rdfs:symbols>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Carnian_Age -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Carnian_Age">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Age"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Late_Triassic"/>
        <rdfs:ChineseName>卡尼期</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Cenozoic -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Cenozoic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Era"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Phanerozoic"/>
        <rdfs:ChineseName>新生代</rdfs:ChineseName>
        <rdfs:comment>新生代（Cenozoic） 是地球历史上目前最新的一个地质时代，它从6600万年前开始一直持续到今天。随着白垩纪﹣古近纪灭绝事件的发生，中生代结束，新生代开始。</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Cisuralian -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Cisuralian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Permian"/>
        <rdfs:ChineseName>乌拉尔世</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Cretaceous -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Cretaceous">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Mesozoic"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Period"/>
        <rdfs:ChineseName>白垩纪</rdfs:ChineseName>
        <rdfs:comment>白垩纪位于侏罗纪和古近纪之间，约1亿4550万年（误差值为400万年）前至6550万年前（误差值为30万年），发生在白垩纪末的灭绝事件，是中生代与新生代的分界。</rdfs:comment>
        <rdfs:seeAlso xml:lang="de">Kreidezeit</rdfs:seeAlso>
        <rdfs:symbols>K</rdfs:symbols>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Dapingian -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Dapingian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Age"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Middle_Ordovician"/>
        <rdfs:ChineseName>大坪期</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Darriwilian -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Darriwilian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Age"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Middle_Ordovician"/>
        <rdfs:ChineseName>达瑞威尔期</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Devonian -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Devonian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Paleozoic"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Period"/>
        <rdfs:ChineseName>泥盆纪</rdfs:ChineseName>
        <rdfs:comment>开始于同位素年龄419.2百万年（Ma），结束于358.9百万年（Ma）</rdfs:comment>
        <rdfs:symbols>D</rdfs:symbols>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Early_Devonian -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Early_Devonian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Devonian"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch"/>
        <rdfs:ChineseName>早泥盆世</rdfs:ChineseName>
        <rdfs:comment>泥盆纪早期从同位素年龄419.2±2.8百万年（Ma）持续至393.3±2.5百万年（Ma），始于洛赫考夫期（419.2±2.8 - 410.8±2.5），随后是布拉格期（410.8±2.8 - 407.6±2.5），然后是埃姆斯期，这个时期一直持续至中泥盆世起始的393.3±2.7百万年前</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Early_Ordovician -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Early_Ordovician">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Ordovician"/>
        <rdfs:ChineseName>早奥陶世</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Early_Triassic -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Early_Triassic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Triassic"/>
        <rdfs:ChineseName>早三叠世</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Eocene -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Eocene">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Paleogene"/>
        <rdfs:ChineseName>始新世</rdfs:ChineseName>
        <rdfs:comment>大约开始于5600万年前-终于3400万年前</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Eon -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Eon">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Interval"/>
        <rdfs:ChineseName xml:lang="zh">元</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Interval"/>
        <rdfs:ChineseName xml:lang="zh">世</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Era -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Era">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Interval"/>
        <rdfs:ChineseName xml:lang="zh">代</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Floian -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Floian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Age"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Early_Ordovician"/>
        <rdfs:ChineseName>弗洛期</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Geological_Age_Unit -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Geological_Age_Unit">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Geological_Time"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Geological_Time -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Geological_Time"/>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Guadalupian -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Guadalupian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Permian"/>
        <rdfs:ChineseName>瓜德鲁普世</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Hirnantian -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Hirnantian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Age"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Late_Ordovician"/>
        <rdfs:ChineseName>赫南特期</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Holocene -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Holocene">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Quaternary_Period"/>
        <rdfs:ChineseName>全新世</rdfs:ChineseName>
        <rdfs:ChineseName>全新世从11700年前新仙女木期结束开始[1]，下分格陵兰期、诺斯格瑞比期和梅加拉亚期三个时期。根据传统的地质学观点，全新世一直持续至今，但也有人提出第一次工业革命后应该另分为人类世，因此全新世应只包括人类前工业时期。</rdfs:ChineseName>
        <owl:priorVersion>Alluvium</owl:priorVersion>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Induan_Age -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Induan_Age">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Age"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Early_Triassic"/>
        <rdfs:ChineseName>印度期</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Interval -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Interval">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Geological_Age_Unit"/>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Jurassic -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Jurassic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Mesozoic"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Period"/>
        <rdfs:ChineseName>侏罗纪</rdfs:ChineseName>
        <rdfs:comment>2亿130万年前到1亿4500万年前</rdfs:comment>
        <rdfs:symbols>J</rdfs:symbols>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Katian -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Katian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Age"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Late_Ordovician"/>
        <rdfs:ChineseName>凯迪期</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Ladinian_Age -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Ladinian_Age">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Age"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Middle_Triassic"/>
        <rdfs:ChineseName>拉丁期</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Late_Devonian -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Late_Devonian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Devonian"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch"/>
        <rdfs:ChineseName>晚泥盆世</rdfs:ChineseName>
        <rdfs:comment>晚泥盆世开始于弗拉斯期（382.7±2.8 - 372.2±2.5）</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Late_Ordovician -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Late_Ordovician">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Ordovician"/>
        <rdfs:ChineseName>晚奥陶世</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Late_Triassic -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Late_Triassic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Triassic"/>
        <rdfs:ChineseName>晚三叠世</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Llandovery -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Llandovery">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Silurian"/>
        <rdfs:ChineseName>兰多维列世</rdfs:ChineseName>
        <rdfs:comment>开始于4.44亿年前，结束于4.33亿年前</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Lopingian -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Lopingian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Permian"/>
        <rdfs:ChineseName>乐平世</rdfs:ChineseName>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Ludlow_epoch -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Ludlow_epoch">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Epoch"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Silurian"/>
        <rdfs:ChineseName>罗德洛世</rdfs:ChineseName>
        <rdfs:comment>427.4±0.5百万年前至423.0±2.3百万年前</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Mesozoic -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Mesozoic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Era"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Phanerozoic"/>
        <rdfs:ChineseName>中生代</rdfs:ChineseName>
        <rdfs:comment>中生代的年代为2.51亿年前至6600万年前，开始于二叠纪-三叠纪灭绝事件，结束于白垩纪-第三纪灭绝事件为止，前后横跨1.8亿年。</rdfs:comment>
        <rdfs:seeAlso>Age of the Reptiles</rdfs:seeAlso>
        <owl:priorVersion>Secondary</owl:priorVersion>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Middle_Devonian -->

    <owl:Class rdf:about="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Middle_Devonian">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/wli/ontologies/2023/Geological_Time#Devonian"/>
